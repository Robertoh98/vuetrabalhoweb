!\");\n        });\n      }\n\n      this.filmes = {};\n      this.listar();\n    },\n    editar: function editar(filme) {\n      this.filme = filme;\n    },\n    excluir: function excluir(filme) {\n      Filme.delete(filme).then(alert('Excluido com sucesso!'));\n      this.listar();\n    }\n  }\n};",{"version":3,"sources":["TableFilmes.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0CA,OAAA,KAAA,MAAA,mBAAA;AAEA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,KAAA,EAAA;AACA,QAAA,MAAA,EAAA,EADA;AAEA,QAAA,UAAA,EAAA,EAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OADA;AAMA,MAAA,MAAA,EAAA;AANA,KAAA;AAQA,GAVA;AAWA,EAAA,OAXA,qBAWA;AACA,SAAA,MAAA;AACA,GAbA;AAcA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AAAA;;AACA,MAAA,KAAA,CAAA,MAAA,GAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAFA;AAGA,KALA;AAMA,IAAA,MANA,oBAMA;AACA,UAAA,CAAA,KAAA,KAAA,CAAA,MAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,KAAA,KAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,WAAA,QAAA,CAAA,IAAA,CAAA,MAAA,GAAA,sBAAA,CAAA;AACA,SAFA;AAGA,OAJA,MAIA;AACA,QAAA,KAAA,CAAA,SAAA,CAAA,KAAA,KAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,WAAA,QAAA,CAAA,IAAA,CAAA,MAAA,GAAA,2BAAA,CAAA;AACA,SAFA;AAGA;;AACA,WAAA,MAAA,GAAA,EAAA;AACA,WAAA,MAAA;AACA,KAlBA;AAmBA,IAAA,MAnBA,kBAmBA,KAnBA,EAmBA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,KArBA;AAsBA,IAAA,OAtBA,mBAsBA,KAtBA,EAsBA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,KAAA,EAAA,IAAA,CAAA,KAAA,CAAA,uBAAA,CAAA;AACA,WAAA,MAAA;AACA;AAzBA;AAdA,CAAA","sourcesContent":["<template>\n    <div class=\"container-fluid\">\n        <div id=\"users\">\n            <h1><center>Filmes disponíveis</center></h1>\n            <table class=\"table table-hover\">\n                <thead>\n                <tr>\n                    <td>Cód.</td>\n                    <td>Título</td>\n                    <td>Tipo</td>\n                    <td>Quantidade</td>\n                    <td>Ações</td>\n                </tr>\n                </thead>\n                <tbody>\n                <tr v-for=\"filme of filmes\" :key=\"filme.codigo\">\n                    <td> {{ filme.codigo }} </td>\n                    <td>{{ filme.titulo }}</td>\n                    <td>{{ filme.tipo }}</td>\n                    <td>{{ filme.quantidade }}</td>\n                    <td><button type=\"button\" @click=\"editar(filme)\"><i class=\"fa fa-edit\"></i></button>\n                        <button type=\"button\" @click=\"excluir(filme)\"><i class=\"fa fa-trash\"></i></button></td>\n                </tr>\n                </tbody>\n            </table>\n        </div>\n        <form @submit.prevent=\"salvar\" class=\"form-inline\">\n            <div class=\"form-group\">\n                <input type=\"text\" v-model=\"filme.titulo\" placeholder=\"Nome\" class=\"form-control margin\">\n            </div>\n            <div class=\"form-group\">\n                <input type=\"text\" v-model=\"filme.tipo\" placeholder=\"Tipo\" class=\"form-control margin\">\n            </div>\n            <div class=\"form-group\">\n                <input type=\"number\" v-model=\"filme.quantidade\" placeholder=\"Quantidade\" class=\"form-control margin\">\n            </div>\n            <button type=\"submit\" class=\"btn btn-primary\">Enviar</button>\n        </form>\n    </div>\n</template>\n\n<script>\n    import Filme from '../services/filme'\n\n    export default {\n        data(){\n            return {\n                filme:{\n                  titulo:'',\n                  quantidade:'',\n                  tipo:''\n                },\n                filmes:[]\n            }\n        },\n        mounted(){\n            this.listar()\n        },\n        methods:{\n            listar(){\n                Filme.listar().then(response => {\n                    this.filmes = response.data\n                })\n            },\n            salvar(){\n                if(!this.filme.codigo){\n                    Filme.salvar(this.filme).then(response => {\n                        alert(\"Filme \" + response.data.titulo + \" salvo com sucesso!!\")\n                    })\n                }else{\n                    Filme.atualizar(this.filme).then(response=>{\n                        alert(\"Filme \" + response.data.titulo + \" atualizado com sucesso!!\")\n                    })\n                }\n                this.filmes = {}\n                this.listar();\n            },\n            editar(filme){\n                this.filme = filme\n            },\n            excluir(filme){\n                Filme.delete(filme).then(alert('Excluido com sucesso!'))\n                this.listar()\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .margin{\n        margin-right: 2px;\n        margin-left: 2px;\n    }\n</style>"],"sourceRoot":"src/components"}]}